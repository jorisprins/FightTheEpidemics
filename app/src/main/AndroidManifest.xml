<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nl.windesheim.fighttheepidemics" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/redcross_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".HealthActivity" ></activity>
        <activity android:name=".MySettingActivity" ></activity>
        <activity android:name=".HealthCheckActivity" ></activity>
        <activity android:name=".HealthReportActivity" ></activity>
        <activity android:name=".HealthMoreinfoActivity" ></activity>
        <activity android:name=".SignInActivity" ></activity>

    </application>

</manifest>
